{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "crons": [
    {
      "path": "/apis/v1/cron/update-images",
      "schedule": "0 0 * * *"
    }
  ]
}
